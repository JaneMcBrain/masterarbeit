.container {
    padding-left: 10%;
    padding-right: 10%;
    padding-top: 10%;
    padding-bottom: 10%;
    left: auto;
    top: auto;
    right: auto;
    bottom: auto;
    background-color: rgb(233, 233, 233);
    position: relative;
    height: 100%;
}

.detail-container {
    padding-left: 10%;
    padding-right: 10%;
    padding-top: 5%;
    padding-bottom: 10%;
    left: auto;
    top: auto;
    right: auto;
    bottom: auto;
    background-color: rgb(233, 233, 233);
    position: relative;
    height: 100%;
}

.button {
    font-size: 32px;
    color: rgb(55, 55, 55);
    -unity-background-image-tint-color: rgb(217, 217, 217);
    background-color: rgb(255, 255, 255);
    padding: 30px;
    -unity-font-style: bold;
    -unity-background-scale-mode: scale-to-fit;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    flex-basis: auto;
}

.button.is-active {
    background-color: rgb(29, 175, 255);
}

.button__icon {
    position: absolute;
    right: 20px;
    bottom: 20px;
    width: 40px;
    height: 40px;
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 20px;
    padding-bottom: 20px;
}

.icon-button-std {
    width: 64px;
    height: 64px;
}

.icon-button {
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
}

.footer-icon {
    height: 100%;
    width: 20%;
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 20px;
    padding-bottom: 20px;
    background-color: rgba(188, 188, 188, 0);
}

.icon-button__icon {
    width: 100%;
    height: 100%;
    -unity-background-scale-mode: scale-to-fit;
}

.icon-button__icon--info {
    background-image: url('project://database/Assets/Icons/info.png?fileID=2800000&guid=6786ab31598084715a9c71f92cd36e88&type=3#info');
    -unity-background-scale-mode: scale-to-fit;
}

.icon-button__icon--info:hover {
    background-image: url('project://database/Assets/Icons/info-primary.png?fileID=2800000&guid=c92969cb30f994b3f96f89f7735c74f7&type=3#info-primary');
}

.icon-button__icon--info.is-active {
