.input {
    -unity-background-image-tint-color: rgb(194, 223, 255);
}

.card-list {
    margin-top: 32px;
    height: 100%;
}

.card-list-item {
    flex-direction: row;
    height: 140px;
    flex-wrap: nowrap;
    align-items: center;
    justify-content: flex-start;
    padding-left: 15px;
    padding-right: 15px;
    padding-top: 15px;
    padding-bottom: 15px;
    background-color: rgb(255, 255, 255);
    margin-bottom: 8px;
}

.card-list-item__image {
    width: 35%;
    height: 100%;
    margin-right: 0;
    margin-left: 0;
    margin-top: 0;
    margin-bottom: 0;
    align-items: auto;
    justify-content: flex-start;
    -unity-background-scale-mode: scale-and-crop;
}

.card-list-item__text {
    width: 100%;
    padding-left: 20px;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
}

.card-list-item__image--location {
    background-image: resource('Sprites/default_location');
}

.card-list-item__image--tour {
    background-image: resource('Sprites/Tours/default_tour');
}

.card-list-item:hover {
    background-color: rgb(221, 221, 221);
}

.detail-header {
    min-height: 20%;
    height: 20%;
    width: 100%;
    top: 0;
}

.detail-header__button {
    background-color: rgb(255, 255, 255);
}

.detail-switch {
    flex-direction: row;
}

.detail-switch__content {
    display: none;
    width: 100%;
    height: 100%;
}

.detail-switch__content.is-active {
    display: flex;
}

.circle {
    border-top-left-radius: 50%;
    border-bottom-left-radius: 50%;
    border-top-right-radius: 50%;
    border-bottom-right-radius: 50%;
}

.show{
    display: flex;
}
